<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".src.menu.MenuActivity3">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="94dp"
        android:background="#FFFFFF"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/menu3_next_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">


        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/menu3_btn1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            android:background="@drawable/survey_btn"
            android:fontFamily="@font/pretendard_regular"
            android:padding="12dp"
            android:stateListAnimator="@null"
            android:text="모태신앙"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationZ="8dp"
            app:elevation="8dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/menu3_btn2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            android:background="@drawable/survey_btn"
            android:fontFamily="@font/pretendard_regular"
            android:padding="12dp"
            android:stateListAnimator="@null"
            android:text="주변인의 권유"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationZ="8dp"
            app:elevation="8dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/menu3_btn3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            android:background="@drawable/survey_btn"
            android:fontFamily="@font/pretendard_regular"
            android:padding="12dp"
            android:stateListAnimator="@null"
            android:text="마음의 안정"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationZ="8dp"
            app:elevation="8dp" />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/menu3_btn4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="12dp"
            android:background="@drawable/survey_btn"
            android:fontFamily="@font/pretendard_regular"
            android:padding="12dp"
            android:stateListAnimator="@null"
            android:text="가입한 적 없음"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold"
            android:translationZ="8dp"
            app:elevation="8dp" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="22dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="기독교에 왜 가입을 하셨나요?"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu3_back_btn" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu3_next_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="71dp"
        android:background="@drawable/next_btn"
        android:fontFamily="@font/pretendard_regular"
        android:text="다음으로"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3" />

    <ImageButton
        android:id="@+id/menu3_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="85dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>