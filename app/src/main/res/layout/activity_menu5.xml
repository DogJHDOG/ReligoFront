<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".src.menu.MenuActivity4">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="어떤 교회를 원하시나요?"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.15"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="136dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="(최대 5개 선택)"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.2" />

    <Button
        android:id="@+id/button6"
        android:layout_width="350dp"
        android:layout_height="55dp"
        android:layout_marginBottom="80dp"
        android:background="@drawable/survey_move_btn"
        android:text="이전으로"
        android:fontFamily="@font/pretendard_regular"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="132dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#가까운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button7"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#활기찬"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button9"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#시간이 잘맞은"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#따뜻한"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#모임이 많은"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button92"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#편안함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#다양한"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#솔직함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button92"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#행사"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="8"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#유아부 청소년부"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#다채로운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button92"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#개인공간"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#궁금함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#축제"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button92"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#절실함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#여유로운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#새벽기도"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <Button
                android:id="@+id/button92"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#열정적"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>