<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".src.menu.MenuActivity6">

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="49dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/menu7_next_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout1">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="자신에서 엄격하고 스스로\n규칙을 가진다."
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:checkedButton="@+id/btnRadio2"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radio2_1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio2_2"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_margin="16dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio2_3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio2_4"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio2_5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

        </RadioGroup>

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView6"

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="전혀아니다"
                android:textAlignment="textStart"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="보통이다"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="매우그렇다"
                android:textAlignment="viewEnd"
                android:textColor="#000000"
                android:textSize="12sp" />
        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="4dp"
        android:layout_marginBottom="10dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu7_back_btn">

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/pretendard_regular"
            android:text="다른 사람이나 집단에 관용적이다."
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="20sp"
            android:textStyle="bold" />

        <RadioGroup
            android:layout_width="match_parent"
            android:layout_height="90dp"
            android:checkedButton="@+id/btnRadio1"
            android:orientation="horizontal">

            <RadioButton
                android:id="@+id/radio1_1"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio1_2"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_margin="16dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio1_3"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio1_4"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

            <RadioButton
                android:id="@+id/radio1_5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_margin="16dp"
                android:layout_marginBottom="5dp"
                android:background="@drawable/radio_btn"
                android:button="@android:color/transparent"
                android:stateListAnimator="@null"
                android:translationZ="5dp"
                app:elevation="5dp" />

        </RadioGroup>

        <LinearLayout
            android:layout_width="400dp"
            android:layout_height="25dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="전혀아니다"
                android:textAlignment="textStart"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="보통이다"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/pretendard_regular"
                android:text="매우그렇다"
                android:textAlignment="viewEnd"
                android:textColor="#000000"
                android:textSize="12sp" />
        </LinearLayout>

    </LinearLayout>

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu7_next_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/next_btn"
        android:fontFamily="@font/pretendard_regular"
        android:text="다음으로"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout2" />

    <ImageButton
        android:id="@+id/menu7_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="35dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_icon" />

</androidx.constraintlayout.widget.ConstraintLayout>