<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_church_imageview1"
    android:layout_width="match_parent"
    android:layout_marginTop="30dp"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_marginLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/home_church_item1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                app:layout_constraintTop_toTopOf="@id/home_church_item1"
                app:layout_constraintStart_toStartOf="@+id/home_church_item1"
                android:id="@+id/home_church_item1_img"
                android:src="@drawable/home_churchimg_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="20dp"
                android:id="@+id/home_church_item1_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/home_church_item1"
                android:fontFamily="@font/pretendard_medium"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item1_img"
                android:text="목동교회"
                android:textSize="20sp"/>

            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                app:layout_constraintTop_toBottomOf="@+id/home_church_item1_text"
                android:id="@+id/home_church_item1_location_picker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/home_location_picker"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item1_img"/>
            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/home_church_item1_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="서울특별시 양천 목동 중앙본로 51-16"
                android:fontFamily="@font/pretendard_regular"
                android:textSize="10sp"
                android:layout_marginLeft="5dp"
                app:layout_constraintTop_toBottomOf="@id/home_church_item1_text"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item1_location_picker"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/home_church_item1_view_more"
                app:layout_constraintRight_toRightOf="@+id/home_church_item1"
                app:layout_constraintBottom_toBottomOf="@id/home_church_item1"
                android:layout_marginTop="5dp"
                android:src="@drawable/home_church_view_more"/>


        </androidx.constraintlayout.widget.ConstraintLayout>



    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="50dp"/>

    <LinearLayout
        android:layout_marginLeft="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/home_church_item2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView
                app:layout_constraintTop_toTopOf="@id/home_church_item2"
                app:layout_constraintStart_toStartOf="@+id/home_church_item2"
                android:id="@+id/home_church_item2_img"
                android:src="@drawable/home_churchimg_background"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>
            <TextView
                android:layout_marginLeft="20dp"
                android:fontFamily="@font/pretendard_medium"
                android:id="@+id/home_church_item2_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="@id/home_church_item2"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item2_img"
                android:text="목동교회"
                android:textSize="20sp"/>


            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                app:layout_constraintTop_toBottomOf="@+id/home_church_item2_text"
                android:id="@+id/home_church_item2_location_picker"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/home_location_picker"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item2_img"/>
            <TextView
                android:layout_marginTop="10dp"
                android:id="@+id/home_church_item2_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="서울특별시 양천 목동 중앙본로 51-16"
                android:fontFamily="@font/pretendard_regular"
                android:textSize="10sp"
                android:layout_marginLeft="5dp"
                app:layout_constraintTop_toBottomOf="@id/home_church_item2_text"
                app:layout_constraintLeft_toRightOf="@+id/home_church_item2_location_picker"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/home_church_item2_view_more"
                app:layout_constraintRight_toRightOf="@+id/home_church_item2"
                app:layout_constraintBottom_toBottomOf="@id/home_church_item2"
                android:layout_marginTop="5dp"
                android:src="@drawable/home_church_view_more"/>


        </androidx.constraintlayout.widget.ConstraintLayout>



    </LinearLayout>

</LinearLayout>