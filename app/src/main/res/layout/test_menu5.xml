<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".src.menu.MenuActivity4">

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="2dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="어떤 교회를 원하시나요?"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu5_back_btn" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/pretendard_regular"
        android:text="(최대 5개 선택)"
        android:textColor="#000000"
        android:textSize="13sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/linearLayout5"
        app:layout_constraintStart_toStartOf="@+id/textView1"
        app:layout_constraintTop_toBottomOf="@+id/textView1" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu5_next_btn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/next_btn"
        android:fontFamily="@font/pretendard_regular"
        android:text="다음으로"
        android:textColor="#FFFFFF"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout5" />

    <LinearLayout
        android:id="@+id/linearLayout5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="77dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/menu5_next_btn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#가까운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#활기찬"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#시간이 잘맞은"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#따뜻한"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#모임이 많은"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#편안함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn7"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#다양한"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#솔직함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn9"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#행사"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn10"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="8"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#유아부 청소년부"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn11"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#다채로운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#개인공간"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="2dp"
            android:layout_marginRight="50dp"
            android:layout_marginBottom="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn13"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#궁금함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn14"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#축제"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn15"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#절실함"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="45dp"
            android:layout_margin="2dp"
            android:orientation="horizontal">

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#여유로운"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn17"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="7"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#새벽기도"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/menu5_btn18"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_margin="5dp"
                android:layout_weight="1.5"
                android:background="@drawable/survey_btn"
                android:fontFamily="@font/pretendard_regular"
                android:stateListAnimator="@null"
                android:text="#열정적"
                android:textSize="15sp"
                android:textStyle="bold"
                android:translationZ="3dp"
                app:elevation="3dp" />
        </LinearLayout>
    </LinearLayout>

    <ImageButton
        android:id="@+id/menu5_back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="30dp"
        android:layout_marginBottom="85dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/textView1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/back_icon" />

    <CheckBox
        android:id="@+id/checkBox"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CheckBox"
        tools:layout_editor_absoluteX="138dp"
        tools:layout_editor_absoluteY="67dp"
        />

</androidx.constraintlayout.widget.ConstraintLayout>